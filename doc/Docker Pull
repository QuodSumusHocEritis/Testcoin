curl -fsSL https://download.docker.com/linux/ubuntu/gpg | sudo apt-key add -
add-apt-repository "deb [arch=amd64] https://download.docker.com/linux/ubuntu $(lsb_release -cs) stable"
apt-get update
apt-cache policy docker-ce
apt-get install -y docker-ce
docker pull quodsumushoceritis/testcoin_network_assets:testcoin-server1_1.0
docker pull quodsumushoceritis/testcoin_network_assets:testcoin-server2_1.0
docker pull quodsumushoceritis/testcoin_network_assets:testcoin-genericminer_1.0
